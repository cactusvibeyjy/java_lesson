package integerVariable;

public class Literal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a =101;
		int b =0146;
		int c =0x1C2B;
		int d =0b1111;
		long l =12314445L;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	}

}
