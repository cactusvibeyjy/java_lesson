package 생성자;

public class Person {
	private String name;
	private double height;
	//기본 생성자는 생략 가능
	public Person() {
		// 기본 생성자
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getHeight() {
		return height;
	}
	public void setHeight(double height) {
		this.height = height;
	}
	
	
	
}
