package application;

public class 빈문자열 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// blank VS. empty
		String myText = "      \n";
		System.out.println("빈문자열 확인 :" + myText.isEmpty()); //빈문자열만 확인
		System.out.println("빈문자열 확인 :" + myText.isBlank()); //공백제외하고 빈문자열 확인
		
		
	}

}
