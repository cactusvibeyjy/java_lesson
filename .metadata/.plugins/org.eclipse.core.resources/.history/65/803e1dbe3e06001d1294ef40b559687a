package application;

public class 연산자위치 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//증감연사자의 위치에 따라 먼저 증감하고 할당 (=), 할당 (=)하고 증감
		int cats =5;
		System.out.println(cats++); //할당 먼저 ->증강
		System.out.println(cats);
		
		int dogs = 3;
		System.out.println(++dogs);
	}

}
