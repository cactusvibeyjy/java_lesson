		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		int a = 24;
		int b = 6;
		int 곱하기 = a*b;
		int 나누기 = a/6;
		int 나머지 = a%b;
		//System.out.println(rest);
