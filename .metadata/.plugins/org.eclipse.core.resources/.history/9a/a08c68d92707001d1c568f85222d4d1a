package getterSetter;

public class App {

	public static void main(String[] args) {
		Person p1 = new Person();
		p1.setName ("펭수");
		p1.setAge(7);
		System.out.println(p1.getName());
		
		Person p2 = new Person();
		p2.setName("Sam");
		p1.setAge(17);
		System.out.println(p2.getName());
		System.out.println(p2.getAge());
		
	}

}
